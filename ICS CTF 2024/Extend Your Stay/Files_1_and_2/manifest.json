{
    "manifest_version": 3,
    "name": "No More Rodents",
    "description": "Are you scared of rodents? Creeped out by their weird little teeth? NO MORE!",
    "version": "1.7",
    "permissions": [
        "tabs", "webNavigation", "activeTab", "scripting"
    ],
    "host_permissions": [
        "*://*/"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "16": "logo_16.png",
        "32": "logo_32.png",
        "48": "logo_48.png",
        "128": "logo_128.png"
    }
  }
