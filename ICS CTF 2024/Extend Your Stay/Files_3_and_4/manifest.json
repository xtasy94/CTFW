{
    "manifest_version": 3,
    "name": "Even More Birds",
    "description": "Everybody loves birds! Increase the birds!",
    "version": "1.7",
    "permissions": [
        "tabs", "webNavigation", "activeTab", "scripting"
    ],
    "host_permissions": [
        "*://*/"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "16": "logo_16.png",
        "32": "logo_32.png",
        "48": "logo_48.png",
        "128": "logo_128.png"
    }
  }
